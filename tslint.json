{
  "extends": "tslint:recommended",
  "rulesDirectory": [],
  "rules": {
    "no-shadowed-variable": false,
    "no-empty": [
      false
    ],
    "prefer-readonly": true,
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "object-literal-sort-keys": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      {
        "limit": 140,
        "ignore-pattern": "^import |//"
      }
    ],
    "member-ordering": [
      false
    ],
    "arrow-parens": [
      false
    ],
    "interface-name": [
      false
    ],
    "no-non-null-assertion": false,
    "curly": [
      true,
      "ignore-same-line"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "quotemark": [
      true,
      "single"
    ]
  }
}